<%= simple_form_for @feedstock_output, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :date, as: :string  %>
  <%= f.input :employee, as: :string  %>
  
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                feedstock_outputs_path, :class => 'btn' %>
  </div>
<% end %>
