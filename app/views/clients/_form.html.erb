<%= simple_form_for(@client) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :cnpj %>
    <%= f.input :cpf %>
    <%= f.input :social_reason %>
    <%= f.input :contact %>
    <%= f.input :email %>
    <%= f.input :phone1 %>
    <%= f.input :phone2 %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
